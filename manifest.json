{
  "name": "Edge Plus",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "",
  "browser_action": {
    "default_title": "Edge Plus"
  },
  "permissions": [
    "https://*/*",
    "tabs"
  ],
  "content_scripts" : [{
    "js": ["modules/fw/fw.js"],
    "css": ["modules/fw/fw.css"],
    "matches": ["<all_urls>"],
    "run_at": "document_idle"
  }]
}